{
    "version": "1.1.0",
    "name": "sysvale/validation-rules",
    "description": "A Laravel library with useful custom rules",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jedson Melo",
            "email": "jedson.melo@sysvale.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/validation": "~5.4|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "ext-zip": "*",
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^8.5.8",
        "orchestra/testbench": "^5.8"
    },
    "autoload": {
        "psr-4": {
            "Sysvale\\ValidationRules\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sysvale\\ValidationRules\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sysvale\\ValidationRules\\Providers\\ServiceProvider"
            ]
        }
    }
}
