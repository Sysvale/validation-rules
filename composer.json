{
    "version": "1.0.0",
    "name": "sysvale/zip-with-xml-validations",
    "description": "A package to help to validate zip file that contains a xml file",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jedson Melo",
            "email": "jedson.melo@sysvale.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/validation": "~5.4|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "ext-zip": "*",
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^8.5.8|^9.3.3",
        "orchestra/testbench": "^5.8"
    },
    "autoload": {
        "psr-4": {
            "Sysvale\\ZipWithXMLValidations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sysvale\\ZipWithXMLValidations\\Tests\\": "tests/"
        }
    }
}
